{
  "name": "dncash-io",
  "version": "1.2.1",
  "description": "dncash.io core API service",
  "main": "app.ts",
  "scripts": {
    "start": "node dist/app.js",
    "develop": "tsc && concurrently \"tsc -w\" \"nodemon dist/app.js\"",
    "build": "tsc",
    "postinstall": "tsc",
    "test": "tsc && mocha --exit dist/test",
    "deploy": "tsc && node dist/awsdeploy.js",
    "createDB": "mysql -udncashio -pdncashio dncashio < db/scripts/drop.sql && mysql -udncashio -pdncashio dncashio < db/scripts/create.sql && mysql -udncashio -pdncashio dncashio < db/scripts/populate.sql"
  },
  "repository": {
    "type": "git",
    "url": "https://tom.wincor-nixdorf.com/repos/dncash-io"
  },
  "author": "Thorsten Liese",
  "license": "ISC",
  "devDependencies": {
    "@types/chai-http": "^3.0.5",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.18",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "concurrently": "^4.1.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.9",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "base32": "0.0.6",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "express-ws": "^4.0.0",
    "form-data": "^2.3.3",
    "generate-password": "^1.4.1",
    "jsonwebtoken": "^8.4.0",
    "morgan": "^1.9.1",
    "mqtt": "^2.18.8",
    "mysql": "^2.16.0",
    "nocache": "^2.0.0",
    "node-fetch": "^2.3.0",
    "node-forge": "^0.7.6",
    "node-schedule": "^1.3.1",
    "redis": "^2.8.0",
    "speakeasy": "^2.0.0",
    "swagger-jsdoc": "^3.2.6",
    "swagger-ui-express": "^4.0.2",
    "uuid": "^3.3.2",
    "winston": "^2.4.4"
  }
}
