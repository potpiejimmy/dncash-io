{
  "name": "dncash-io",
  "version": "1.5.0",
  "description": "dncash.io core API service",
  "main": "app.ts",
  "scripts": {
    "start": "node dist/app.js",
    "develop": "tsc && concurrently \"tsc -w\" \"nodemon dist/app.js\"",
    "build": "tsc",
    "postinstall": "tsc",
    "test": "tsc && mocha --exit dist/test",
    "deploy": "tsc && node dist/awsdeploy.js",
    "createDB": "mysql -udncashio -pdncashio dncashio < db/scripts/drop.sql && mysql -udncashio -pdncashio dncashio < db/scripts/create.sql && mysql -udncashio -pdncashio dncashio < db/scripts/populate.sql"
  },
  "repository": {
    "type": "git",
    "url": "https://tom.wincor-nixdorf.com/repos/dncash-io"
  },
  "author": "Thorsten Liese",
  "license": "ISC",
  "devDependencies": {
    "@types/chai-http": "^3.0.5",
    "@types/mocha": "^7.0.2",
    "@types/node": "^12.12.30",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "concurrently": "^5.1.0",
    "mocha": "^7.1.0",
    "nodemon": "^2.0.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "base32": "0.0.6",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "express-ws": "^4.0.0",
    "form-data": "^3.0.0",
    "generate-password": "^1.5.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "mosca": "^2.8.3",
    "mqtt": "^3.0.0",
    "mysql": "^2.18.1",
    "nocache": "^2.1.0",
    "node-fetch": "^2.6.0",
    "node-forge": "^0.9.1",
    "node-schedule": "^1.3.2",
    "redis": "^3.0.2",
    "speakeasy": "^2.0.0",
    "swagger-jsdoc": "^3.5.0",
    "swagger-ui-express": "^4.1.3",
    "uuid": "^7.0.2",
    "winston": "^2.4.4"
  }
}
